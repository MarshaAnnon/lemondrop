<br><br>
<h5>Here are all your quotes</h5><br>
    <% @quotes.each do | q | %>
        <div class="card">
            <div class="card-body">
                <p class="card-text"><%= q.content %></p>
            </div>
            <a class="badge badge-light" <%= link_to "Delete Quote", quote_path(q), method: "delete" %></a><br>
            <a class="badge badge-light" <%= link_to "See Book", book_path(q.book), method: "get" %></a>
        </div>
    <% end %>


            