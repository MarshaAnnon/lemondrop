<div class="nav_bar">   
    <% if user_signed_in? %>
        <% if current_user.provider == "" %> 
            <nav>              
                <ul>
                    <a <%= link_to "Home |", root_path(current_user), style: "color: #F4EDE2;" %></a>
                    <a <%= link_to "Back |", :back, style: "color: #F4EDE2;" %></a>
                    <a <%= link_to "Edit Your Profile |", edit_user_registration_path(current_user), style: "color: #F4EDE2;" %></a>
                    <a <%= link_to "Sign Out", destroy_user_session_path, method: :delete, style: "color: #F4EDE2;" %></a>
                </ul>
        <% end %>
        <% else %>
            <ul>
                <a <%= link_to "Sign In |", new_user_session_path, style: "color: #F4EDE2;" %></a>
                <a <%= link_to "Sign Up", new_user_registration_path, style: "color: #F4EDE2;" %></a>
            </ul>
            </nav>
    <% end %>         
</div>